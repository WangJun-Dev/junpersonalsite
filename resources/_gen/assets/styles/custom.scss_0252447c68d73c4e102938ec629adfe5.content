:root{--light: #ffffff;--dark: #000000;--secondary: #474747;--tertiary: #6e6e6e;--visited: #969696;--primary: #ff6224;--gray: #4e4e4e;--lightgray: #f0f0f0;--outlinegray: #dadada;--highlighted: #ffa88643;--background-modifier-cover: rgba(244, 244, 244, 0.64);--link-background-color: #c7c7c731;--link-background-hover-color: #ff9f794a}[saved-theme="dark"]{--light: #24262a;--dark: #ffffff;--secondary: #adadad;--visited: #7d7d7d;--tertiary: #666666;--primary: #89b8ff;--gray: #b8b8b8;--lightgray: #2f3039;--outlinegray: #333333;--highlighted: #89b8ff4f;--background-modifier-cover: rgba(0, 0, 0, 0.149);--link-background-color: #a3a3a336;--link-background-hover-color: #97c1ff5d}audio{width:100%}span.hanchor{position:relative;font-size:.5rem;font-weight:700;opacity:.3;top:-20%;left:-2rem;display:none}.endnote{opacity:0.5;line-height:1.2;font-weight:200;margin:0}.endnote a{font-weight:400}article img{max-width:100%;display:block;margin:0 auto;max-height:75vh}article iframe{box-sizing:border-box}div.highlight{margin-top:1em;margin-bottom:1em}h1{font-size:1.60em;font-weight:800}h2{font-size:1.50em;font-weight:720}h3{font-size:1.40em;font-weight:680}h4{font-size:1.30em;font-weight:640}h5{font-size:1.20em;font-weight:600}h6{font-size:1.10em;font-weight:560}div.badge-group>p{display:flex;gap:0.5em;flex-wrap:wrap}div.badge-group>p img{margin:unset}body.about-creator article h2{font-size:1.35em;margin-top:1em;font-weight:720;font-family:var(--font-header)}body.about-creator article h3{font-size:1.2em;margin-top:1em;font-weight:500;font-family:var(--font-header)}body.resume h1.article-title{display:none}body.resume article .TOC{display:none}body.resume article .text-chunk>a>h2{font-size:1.1em;font-weight:600;font-family:var(--font-header);border-bottom:solid 1px var(--outlinegray);margin-top:3em;margin-bottom:1em;color:var(--secondary)}body.resume article .text-chunk>a>h3{font-size:1em;margin-top:1em;margin-bottom:0.5em;font-weight:650;font-family:var(--font-header);color:var(--secondary)}body.resume article .text-chunk>a>h5{font-size:1em;font-family:var(--font-body);margin-top:1em;margin-bottom:-0em;font-weight:500;color:var(--secondary)}body.resume article .text-chunk>a>h6{font-size:1em;font-family:var(--font-body);margin-top:0em;margin-bottom:0.2em;font-weight:inherit;color:var(--tertiary)}body.resume article table{margin-top:0.2em}body.resume article thead{display:none}body.resume article td,body.resume article th{padding:0.2em 0;border:none;text-align:left;vertical-align:top}body.resume article td a,body.resume article th a{font-weight:inherit}body.resume article a{font-weight:inherit}body.resume article a.internal-link,body.resume article a.internal-link:hover{background-color:transparent;padding:initial;margin:initial;text-decoration:none}body.resume article a.internal-link:hover{text-decoration:underline}body.resume article ul li,body.resume article ol li,body.resume article p,body.resume article td,body.resume article th,body.resume article a{font-weight:350;color:var(--visited)}body.resume article .text-chunk>a{pointer-events:none}body.resume article p,body.resume article ul,body.resume article ol{margin-top:0.2em;margin-bottom:0.2em}body.resume article tr>th:first-child,body.resume article tr>td:first-child{padding-right:2em;min-width:6em}body.resume article strong{font-weight:550}body.atoms .TOC,body.atoms .floating{display:none !important}body.atoms article hr:not(.ahh){overflow:visible;padding:0;margin:64px auto;border:none;color:var(--gray);text-align:center;width:100%;z-index:-999;border-color:var(--gray)}body.atoms article hr:not(.ahh)::after{content:"* * *";z-index:-999;display:inline-block;margin:-1em 0 0.5em;font-size:1.5em;padding:0.5em 1em;background:var(--light);transform:translateY(-0.1em)}body.atoms article img{max-width:500px}blockquote[class*=minimal-callout]{border:var(--outlinegray) 1px solid !important;background-color:unset !important}blockquote[class*=minimal-callout] p:first-child{border-bottom:var(--outlinegray) 1px solid;padding:0.4em 1em !important;border-top-left-radius:5px;background-color:var(--background-modifier-cover)}blockquote[class*=minimal-callout].callout-collapsed>p{border-bottom:0px !important;border-bottom-left-radius:5px}div.portfolio-nav{padding:0.75em 1em;margin:1em 0;border:var(--outlinegray) 1px solid;border-radius:5px;line-height:1.5;font-family:var(--font-header);margin:16px 0;color:var(--tertiary)}div.portfolio-nav a{font-weight:unset;text-decoration:none;background-color:var(--link-background-color);color:var(--tertiary);padding:0 0.1em;margin:auto -0.1em;border-radius:3px}div.portfolio-nav a:hover{color:var(--primary)}div.portfolio-nav b>a{font-weight:800;background-color:#c7c7c750;color:var(--dark)}
