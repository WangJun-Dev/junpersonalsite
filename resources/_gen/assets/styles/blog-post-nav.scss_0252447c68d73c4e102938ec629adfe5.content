.page-time-nav{display:flex;border:var(--outlinegray) 1px solid;border-radius:5px;margin:1em 0}.page-time-nav .next,.page-time-nav .prev{width:50%;padding:0.5em 1em}.page-time-nav .v-bar{background-color:var(--outlinegray);border-left:var(--outlinegray) solid 1px;margin:0.5em 0;width:0}.page-time-nav .next:hover,.page-time-nav .prev:hover{background-color:var(--background-modifier-cover)}.page-time-nav .page-nav-label{font-family:var(--font-header);font-weight:300;font-size:12px;letter-spacing:3px}.page-time-nav .next{text-align:right}.page-time-nav .prev{text-align:left}
